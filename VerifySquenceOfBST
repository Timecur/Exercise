class Solution {
public:
    bool VerifySquenceOfBST(vector<int> sequence) {
        if(sequence.empty()){
            cout << "No" << endl;
            return false;
        }
        int pos = sequence.size()-1;
        for(int i = 0; i < sequence.size()-1; i++){
 
            if(sequence[i] > sequence[sequence.size()-1]){
                pos = i;
            }
            if(sequence[i] < sequence[sequence.size()-1] && i >= pos){
                cout << "No" << endl;
                return false;
            }
        }
        cout << "Yes" << endl;
        return true;
    }
};
